<script>
  function subtitleTyped(){
    const sub = '<%- theme.cover.subtitle instanceof Array ? theme.cover.subtitle.join(';') : theme.cover.subtitle %>'
    console.log(sub)
    window.subtitleTyped = new Typed('.cover-body .top .subtitle #subtitle', {
      strings: sub.split(';'),
      startDelay: <%- theme.plugins.typed.startDelay ? theme.plugins.typed.startDelay : 300 %>,
      typeSpeed: <%- theme.plugins.typed.typeSpeed ? theme.plugins.typed.typeSpeed : 150 %>,
      backSpeed: <%- theme.plugins.typed.backSpeed ? theme.plugins.typed.backSpeed : 50 %>,
      loop: <%- theme.plugins.typed.loop ? theme.plugins.typed.loop : false %>,
    })
  }
  (async() => { 
    await volantis.js("<%- theme.plugins.typed.js %>")
    <% if(theme.cover.subtitle){ %>
      subtitleTyped()
    <%}%>
  })()
</script>